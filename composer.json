{
    "name": "webignition/quoted-string-value-extractor",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/quoted-string-value-extractor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\QuotedStringValueExtractor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\QuotedStringValueExtractor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs src tests --colors --standard=PSR12",
        "static-analysis": "./vendor/bin/phpstan analyse src tests --level=7",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@composer cs",
            "@composer static-analysis",
            "@composer test"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.77",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "sort-packages": true
    }
}
